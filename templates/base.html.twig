<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <!-- <link rel="stylesheet" href="~/../../../bootstrap.min.css"> -->
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
            <a class="navbar-brand" href="{{path('fiche_acceuil')}}" style="padding-left:100px">Acceuil</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            
            <div class="collapse navbar-collapse " id="navbarColor01">
                <ul class="navbar-nav mr-auto">
                {% block navbarLi %}       
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('fiches_with_filtre')}} ">LISTE DE FICHES</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('form_action')}} ">NOUVELLE LISTE</a>
                    </li>
                {% endblock %}
                 </ul>
             </div>
         </nav>

        <div class="container">
            {% block body %} {% endblock %}
    
            {% block javascripts %} {% endblock %}
        </div>
    </body>
        <!-- //  <script src="/fiche-app/jquery compressed-3.2.1.min.js"></script>
        //  <script src="/fiche-app/bootstrap.min.js"></script> -->
</html>
